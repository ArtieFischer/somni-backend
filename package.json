{
  "name": "somni-backend",
  "version": "1.0.0",
  "description": "Somni Backend Service - ElevenLabs Integration Layer",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "NODE_ENV=production node dist/server.js",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "type-check": "tsc --noEmit",
    "ingest-jung": "tsx src/scripts/ingest-jung-texts.ts",
    "ingest-jung-optimized": "node --expose-gc -r tsx/cjs src/scripts/ingest-jung-texts-optimized.ts",
    "ingest-jung-stream": "node --expose-gc --max-old-space-size=2048 -r tsx/cjs src/scripts/ingest-jung-stream.ts",
    "ingest-freud": "node --expose-gc -r tsx/cjs src/scripts/ingest-freud-texts.ts",
    "ingest-freud-single": "node --expose-gc -r tsx/cjs src/scripts/ingest-single-freud-text.ts",
    "ingest-mary": "node --expose-gc -r tsx/cjs src/scripts/rag/ingest-mary-texts.ts",
    "test:rag-api": "tsx src/scripts/test-rag-api.ts",
    "test:rag-debug": "tsx src/scripts/test-rag-debug.ts",
    "test:freud-rag": "tsx src/scripts/test-freud-rag.ts"
  },
  "keywords": [
    "somni",
    "elevenlabs",
    "transcription",
    "supabase"
  ],
  "author": "Somni Team",
  "license": "MIT",
  "dependencies": {
    "@elevenlabs/elevenlabs-js": "^2.2.0",
    "@supabase/supabase-js": "^2.38.0",
    "@xenova/transformers": "^2.17.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "esbuild": "^0.25.5",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "openai": "^5.3.0",
    "sharp": "^0.34.2",
    "tiktoken": "^1.0.21",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.0",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
