# Server Configuration
PORT=3000
NODE_ENV=development

# Supabase Configuration
SUPABASE_URL=your_supabase_url
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_KEY=your_supabase_service_key

# OpenRouter Configuration (for AI models)
OPENROUTER_API_KEY=your_openrouter_api_key

# ElevenLabs Configuration (for TTS)
ELEVENLABS_API_KEY=your_elevenlabs_api_key

# Feature Flags
ENABLE_RAG=false # Set to true to enable RAG features
ENABLE_RAG_JUNG=true # Enable RAG for Jung interpreter (if ENABLE_RAG=true)
ENABLE_RAG_FREUD=false # Enable RAG for Freud interpreter
ENABLE_RAG_NEUROSCIENTIST=false # Enable RAG for Neuroscientist interpreter
ENABLE_RAG_MARY=true

# Debug Features
SHOW_DEBATE_PROCESS=false # Show internal debate process in test mode

# Dream Title Generation
ENABLE_TITLE_GENERATION=true # Enable automatic dream title generation (default: true)
TITLE_GENERATION_MODEL=meta-llama/llama-4-scout:free # Model to use for title generation
TITLE_GENERATION_MAX_TOKENS=20 # Maximum tokens for title generation
TITLE_GENERATION_TEMPERATURE=0.7 # Temperature for title generation (0-1)